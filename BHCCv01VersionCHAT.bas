          hrs = Int(t * 24) : mins = Int((t * 24 - hrs) * 60) : tline$ = "Sunrise " + Str$(hrs) + ":" + Right$("00" + Str$(mins), 2) 'THE PRINT-FORMAT OF THIS TIME OF DAY
          #gw1 "font times_new_roman 8 bold"
          #gw1 "stringwidth? tline$ width"
          #gw1 "place "; xpos + 5; " "; ypo - 11; "; color "; Black$; "; backcolor "; Black$; "; boxfilled "; xpos + 6 + width; " "; ypo 'BLACK BACKGROUND AREA FOR THE TEXT
          #gw1 "color "; Yellow2$ 'MEDIUM YELLOW TEXT COLOR
          #gw1 "place "; xpos + 5; " "; ypo - 2
          #gw1 "|"; tline$
        End If
      End If
      If x >= NewMoonJulDay and x <= HebMonthLastJulDay Then 'FROM THE NEW MOON TO THE LAST hDay OF THE HEBREW MONTH
'''PRINT THE GREGORIAN DATE & JULIAN DAY . . .
        gDay = gDay + 1 'DAY COUNT OF THE CURRENT GREGORIAN MONTH
        If gDay = gMDays + 1 Then
          gDay = 1 : gMonth = gMonth + 1 : gMDays = NextNMGregMonDays
          If gYear = 1582 and gMonth = 11 Then gDayAdj = 0 'RETURN gDayAdj TO 0
          If gMonth > 12 Then gMonth = 1 : gYear = Year + 1
        End If
        If gYear = 0 Then gYear = 1 'NO Year "0"
        If gYear = 1582 and gMonth = 10 and gDay > 4 Then gDayAdj = 10 'ADJUST FOR 10 DAY CALENDAR-SKIP ON OCT 4, 1582
        pgMon = gMonth : pgYear = gYear
        If gDay + gDayAdj > 0 Then
          pDay$ = Str$(gDay + gDayAdj)
        Else 'CORRECT THE DISPLAY DATE WHEN GOING BACK FOR THE NEW MOON DATE MOVES INTO THE PREVIOUS GREGORIAN MONTH
          pgMon = gMonth - 1 : If pgMon = 0 Then pgMon = 12 : pgYear = gYear - 1 : If pgYear = 0 Then pgYear = -1
          Select Case pgMon
          Case 1, 3, 5, 7, 8, 10, 12 : pDay$ = "31"
          Case 2 : pDay$ = "28" : gy = pgYear : If gy < 0 then gy = ABS(gy) + 3
            If gy / 4 = Int(gy / 4) Then pDay$ = "29"
            If gy / 100 = Int(gy / 100) Then pDay$ = "28"
            If gy / 400 = Int(gy / 400) Then pDay$ = "29"
          Case Else : pDay$ = "30"
          End Select
        End If
        pMonth$ = Mid$("Jan Feb Mar Apr May Jun Jul Aug Sep Oct Nov Dec ", pgMon * 4 - 3, 4)
        gEra$ = "BC" : If pgYear > 0 Then gEra$ = "AD"
        If weekDay <> 7 Then gEra$ = gEra$ + "     "
        pline$ = pMonth$ + pDay$ + ", " + Str$(ABS(pgYear)) + gEra$ + "   j" + Str$(x - 1) '*****  THE JULIAN DAY IS 1 LESS THAN x FOR DISPLAY - THE DAY OFFICIALLY CHANGES AT 12:00 NOON  *****
        '''xpos = weekDay * 200 + 158 - Len(pline$) * 3 : ypos = calRow * 130 + 193
        xpos = weekDay * 200 + 166 - Len(pline$) * 3 : ypos = calRow * 130 + 193
        If weekDay = 7 Then xpos = 1532 - Len(pline$) * 3 'SATURDAYS HAVE A NARROWER SPACE TO PRINT THIS INFORMATION
        #gw1 "font times_new_roman 8; color "; Grey4$ 'DARK GREY TEXT COLOR
        #gw1 "place ";xpos; " "; ypos
        #gw1 "|"; pline$
'''CHOOSE THE CORRECT MOONPHASE DATA . . .
        If x = NewMoonJulDay Then MP = 1 : MPDay = NewMoonJulDay : MPToD = NewMoonToD
        If x = Qtr1JulDay Then MP = 2 : MPDay = Qtr1JulDay : MPToD = Qtr1ToD
        If x = FullMoonJulDay Then MP = 3 : MPDay = FullMoonJulDay : MPToD = FullMoonToD
        If x = Qtr3JulDay Then MP = 4 : MPDay = Qtr3JulDay : MPToD = Qtr3ToD
'''PLACE THE SUNSET ICON . . .
        If (x = MPDay and MPToD <= NewMoonSunsetToD) or extendxpos = 1 Then
          If MP = 1 Then 'THIS IS NewMoonJulDay - DRAW THE SUNSET ICON . . .
            xpos = weekDay * 200 - 19 : ypos = calRow * 130 + 180
            pline$ = "SunsetIcon1" : If weekDay = 1 Then pline$ = "SunsetIcon2" 'SunsetIcon2 HAS SUNDAY'S BACKGROUND, WHICH IS DIFFERENT FROM THE OTHER 6 DAYS
            #gw1 "drawbmp "; pline$; " "; xpos; " "; ypos
'''PRINT THE SUNSET TIME OF DAY . . .
            hrs = Int(NewMoonSunsetToD * 24) : mins = Int((NewMoonSunsetToD * 24 - hrs) * 60) : tline$ = "Sunset " + Str$(hrs) + ":" + Right$("00" + Str$(mins), 2) 'THE PRINT-FORMAT OF THIS TIME OF DAY
            #gw1 "font times_new_roman 8 bold"
            #gw1 "stringwidth? tline$ width"
            #gw1 "place "; xpos + 20; " "; ypo - 11; "; color "; Black$; "; backcolor "; Black$; "; boxfilled "; xpos + 21 + width; " "; ypo 'BLACK BACKGROUND AREA FOR THE TEXT
            #gw1 "color "; Yellow2$ 'MEDIUM YELLOW TEXT COLOR
            #gw1 "place "; xpos + 20; " "; ypos - 2
            #gw1 "|"; tline$
          End If
'''PLACE THE 4 MOON PHASE ICONS AND PRINT THEIR TIMES OF DAY . . .
          If MPToD <= NewMoonSunsetToD Then 'THE MOON PHASE OCCURS ON THIS GREGORIAN DAY BETWEEN MIDNIGHT AND SUNSET, ON THIS HEBREW DAY
            xpos = Int(weekDay * 200 + 1 + 52 + MPToD / NewMoonSunsetToD * 147) 'CALCULATING THE ICON'S PIXEL POSITION WITHIN THIS HEBREW PART-DAY - DOES NOT ADJUST FOR HALF THE ICON WIDTH (-9)
            ypos = calRow * 130 + 180
          Else 'THE MOON PHASE OCCURS ON THIS GREGORIAN DAY BETWEEN SUNSET AND MIDNIGHT, ON THE NEXT HEBREW DAY
            xpos = Int(weekDay * 200 + 1 + (MPToD - NewMoonSunsetToD) / (1 - NewMoonSunsetToD) * 52) 'CALCULATING THE ICON'S PIXEL POSITION WITHIN THIS HEBREW PART-DAY - DOES NOT ADJUST FOR HALF THE ICON WIDTH (-9)
            ypos = calRow * 130 + 180
          End If
          xppos = xpos + 9 'ADJUSTED FOR ICON-WIDTH (+9) - THIS VALUE FOR xppos PRINTS THE MPToD TO THE RIGHT OF THE ICON
          hrs = Int(MPToD * 24) : mins = Int((MPToD * 24 - hrs) * 60) : pline$ = Str$(hrs) + ":" + Right$("00" + Str$(mins), 2) 'THE PRINT-FORMAT OF THIS TIME OF DAY
          #gw1 "font times_new_roman 8 bold" : #gw1 "stringwidth? pline$ width"
          If MPToD > NewMoonSunriseToD and extendxpos = 0 Then xppos = xppos - width - 19 'THIS VALUE FOR xppos PRINTS THE MPToD TO THE LEFT OF THE ICON
          If MP = 1 Then fline$ = "NewMoonIcon "
          If MP = 2 Then fline$ = "NewMoonQ1Icon "
          If MP = 3 Then fline$ = "NewMoonFullIcon "
          If MP = 4 Then fline$ = "NewMoonQ3Icon "
          #gw1 "drawbmp "; fline$; xpos - 9; " "; ypos 'DRAW THE MOON PHASE ICON - ICON-WIDTH ADJUSTED (-9)
          #gw1 "place "; xppos; " "; ypos + 5; "; color "; Black$; "; backcolor "; Black$; "; boxfilled "; xppos + 1 + width; " "; ypos + 16 'BLACK BACKGROUND AREA FOR THE TEXT
          #gw1 "place "; xppos; " "; ypos + 14 'ICON-WIDTH ADJUSTED
          #gw1 "color "; Yellow2$ 'MEDIUM YELLOW TEXT COLOR
          #gw1 "|"; pline$
'''PLACE THE DELAYED NEW MOON ICON WITH YELLOW POINTER . . .
          If MP = 2 Then 'WAIT FOR EXTRA DAYS TO BE COMPLETED BEFORE DRAWING THE NewMoonDelayIcon - AVOIDS DRAWING ICONS THAT OVERLAP, IN THE WRONG ORDER
            #gw1 "drawbmp NewMoonDelayIcon "; dxpos - 9; " "; dypos 'ICON-WIDTH ADJUSTED (-9)
          End If
          extendxpos = 0 : MP = 0
        Else 'NOT [(x = MPDay and MPToD <= NewMoonSunsetToD) or extendxpos = 1]
          If x = MPDay and MPToD > NewMoonSunsetToD Then extendxpos = 1
        End If 'END If (x = MPDay and MPToD <= NewMoonSunsetToD) or extendxpos = 1
'''CALCULATE THE NewMoonDelayToD . . .
        If x = NewMoonJulDay Then 'THE CALCULATION IS DONE ONLY ONCE
          FullDaysToIcon = Int(NewMoonToD + DegreesDayDelay)
          NewMoonDelayJulDay = NewMoonJulDay + FullDaysToIcon
          NewMoonDelayToD = NewMoonToD + DegreesDayDelay - FullDaysToIcon
        End If
'''CALCULATE THE PIXEL-POSITION OF THE DELAYED NEW MOON ICON . . .
        If x = NewMoonDelayJulDay Then 'MAKE THIS CALCULATION ONLY ONCE EARLY, BEFORE IT IS PLACED WHEN MP = 2, IN THE CODE ABOVE
          If NewMoonDelayToD <= NewMoonSunsetToD Then 'THE DELAYED NEW MOON OCCURS ON THIS GREGORIAN DAY BETWEEN MIDNIGHT AND SUNSET, ON THIS HEBREW DAY
            dxpos = Int(weekDay * 200 + 1 + 52 + NewMoonDelayToD / NewMoonSunsetToD * 147) 'CALCULATING THE ICON'S PIXEL POSITION WITHIN THIS HEBREW PART-DAY - DOES NOT ADJUST FOR HALF THE ICON WIDTH (-9)
            dypos = calRow * 130 + 180
          Else 'THE DELAYED NEW MOON OCCURS ON THIS GREGORIAN DAY BETWEEN SUNSET AND MIDNIGHT, ON THE NEXT HEBREW DAY
            dxpos = Int((weekDay + 1) * 200 + 1 + (NewMoonDelayToD - NewMoonSunsetToD) / (1 - NewMoonSunsetToD) * 52) 'CALCULATING THE ICON'S PIXEL POSITION WITHIN THIS HEBREW PART-DAY - DOES NOT ADJUST FOR HALF THE ICON WIDTH (-9)
            dypos = calRow * 130 + 180
            If dxpos > 1600 Then dxpos = dxpos - 1400 : dypos = dypos + 130 'AN AFTER-SUNSET SATURDAY PLACEMENT IS MOVED TO THE NEXT HEBREW 1ST DAY OF THE WEEK
          End If
        End If
      End If 'END If x >= NewMoonJulDay and x <= HebMonthLastJulDay
    Next x 'END FOR x = StartCalJulDay TO EndCalJulDay
    #gw1 "flush" 'MAKE THE GRAPHICS "STICK"

WAIT 'WAITING FOR USER INPUT IN ORDER TO PROCEED

