      UNLOADBMP "CalDayBkg02Icon"
      UNLOADBMP "CalDayBkg03Icon"
      UNLOADBMP "CalDayBkg04Icon"
      UNLOADBMP "CalDayBkg05Icon"
      UNLOADBMP "CalDayBkg06Icon"
      UNLOADBMP "CalDayBkg07Icon"
      UNLOADBMP "CalDayBkg08Icon"
      UNLOADBMP "CalDayBkg09Icon"
      UNLOADBMP "CalDaySpc01Icon"
      UNLOADBMP "CalDaySpc02Icon"
      UNLOADBMP "CalDaySpc03Icon"
      Close #gw1
      END 'END THE PROGRAM
    Else
      If continue < 3 Then
        #gw1 "cls" 'CLEAR THE SCREEN TO AVOID POSSIBLE MEMORY ISSUES
        #gw1 "fill "; Black$ 'WEBPAGE BACKGROUND IS BLACK
      End If
      If continue = 1 Then Goto [ReturnForButtonNewMonth] 'THE PROGRAM CONTINUES WITH A NEW BUTTON-CLICKED HebMonth
