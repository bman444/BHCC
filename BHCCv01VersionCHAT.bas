[getNextTimeline]
    tlAdj = 1
[getPrevTimeline]
    If tlAdj = 0 Then tlAdj = -1
    If Year < TLYear(1) Then
      If tlAdj = -1 Then Year = TLYear(29)
      If tlAdj = 1 Then Year = TLYear(1)
    Else
      If Year > TLYear(29) Then
        If tlAdj = -1 Then Year = TLYear(29)
        If tlAdj = 1 Then Year = TLYear(1)
      Else
        For tln = 1 to 29
          If Year = TLYear(tln) Then Year = TLYear(tln + tlAdj) : Exit For
          If Year > TLYear(tln) and Year < TLYear(tln + 1) Then
            If tlAdj = -1 Then Year = TLYear(tln) : Exit For
            If tlAdj = 1 Then Year = TLYear(tln + 1) : Exit For
          End If
        Next tln
      End If
    End If : tlAdj = 0
    HebMonth = 1 : continue = 2 : Goto [ReturnOrExit]

[errorHandler] 'UNEXPECTED ERROR - SHUTDOWN . . . . . .
    pline$ = "  !!!   UNEXPECTED   ERROR   !!!  "
    gw = DisplayWidth / 2 : gh = DisplayHeight / 2 : fontSize = Int(gw / 20)
    continue = 0 : y = 6000 : col$ = Yellow3$

[ReturnOrExit] 'CHANGE PARAMETERS AND RETURN, OR EXIT THE PROGRAM . . . . . .
    If y <> 6000 Then y = 100000 'THE PROGRAM DIDN'T PASS THROUGH [errorHandler] BEFORE ARRIVING HERE
    For x = 1 to y 'DELAY FOR USER'S VISUAL
      If y = 6000 Then 'GIVE THE USER NOTICE OF THE UNEXPECTED ERROR
        If x / 600 = Int(x / 600) Then
          col$ = Yellow3$
        Else
          If x / 400 = Int(x / 400) Then
            col$ = Red3$
          Else
            If x / 200 = Int(x / 200) Then col$ = Blue3$
          End If
        End If
        #gw1 "font times_new_roman "; fontSize; " bold"
        #gw1 "stringwidth? pline$ width"
        #gw1 "place "; gw - width / 2; " "; gh; "; color "; col$; "; backcolor "; Black$
        #gw1 "\"; pline$
      End If
    Next x
    If continue = 0 Then
'CLOSE #gw1 GRAPHICS WINDOW WITH ITS CONTROLS
      UNLOADBMP "MoonPhaseIcon0"
      UNLOADBMP "MoonPhaseIcon9"
